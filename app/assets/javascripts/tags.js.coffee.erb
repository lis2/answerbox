$ ->
  $tags = $("#tags")
  if $tags.length > 0
    
    $tags.change (e) ->
      tag_value = $tags.val()
      filter_route = "<%= Rails.application.routes.url_helpers.filter_questions_path %>"
      $.ajax
        url: filter_route
        type: "POST"
        data:
          tag_name: tag_value
      .error ->
        alert "Error occured"
